@model Indpro.Attendance.WebApp.Models.EmployeeIndexModel

<table class="table table-bordered table-striped">
    <tr>
        <th>EmployeeNo</th>
        <th>Name</th>
        <th>Designation</th>
        <th>Qualification</th>
        <th>DateOfJoin</th>
        <th>Mobile</th>
        <th>Skype</th>
        <th>Email</th>
        <th>IsActive</th>
        <th>Edit</th>
        <th>DeActive</th>
    </tr>
    @foreach (var u in Model.EmployeeList)
    {
        <tr>
            <td>@u.EmployeeNo</td>
            <td>@u.EmployeeName</td>
            <td>@u.EmployeeDesignation</td>
            <td>@u.EmployeeQualification</td>
            <td>@u.EmployeeDOJ</td>
            <td>@u.EmployeeMobileNo</td>
            <td>@u.EmployeeSkypeID</td>
            <td>@u.EmployeeEmailID</td>
            <td>@u.IsActive</td>
            <td>@Html.ActionLink("Edit", "Edit", "Employees", new { id = u.EmployeeID }, null)</td>
            <td>@Html.ActionLink("DeActive", "Delete", "Employees", new { id = u.EmployeeID }, new { onclick = "return confirm('Are you Sure you want to delete this record?')" })</td>
        </tr>
    }
</table>