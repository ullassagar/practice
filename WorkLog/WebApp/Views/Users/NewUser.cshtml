@model WebApp.Models.UserModel
           
@using (Html.BeginForm("ADD", "User", FormMethod.Post))
{
    <table style="text-align:left; width:600px" border="1" cellpadding="2" cellspacing="2">
        @Html.HiddenFor(u=>u.UserID)
        
        <tr>
            <td>
                @Html.LabelFor(u=>u.EmployeeID)
            </td>
            <td>
                @Html.TextBoxFor(u=>u.EmployeeID)

            </td>
        </tr>
        
        <tr>
            <td>
                @Html.LabelFor(u => u.UserName)
            </td>
            <td>
                @Html.TextBoxFor(u => u.UserName)

            </td>
        </tr>
        
        <tr>
            <td>
                @Html.LabelFor(u => u.Password)
            </td>
            <td>
                @Html.TextBoxFor(u => u.Password)

            </td>
        </tr>

    </table>
    <br/>
    <input type="submit" value="submit">
}